{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "bun dev frontend",
      "type": "bun",
      "script": "dev",
      "isBackground": true,
      "problemMatcher": [
        {
          "pattern": [
            {
              "regexp": ".",
              "file": 1,
              "location": 2,
              "message": 3
            }
          ],
          "background": {
            "activeOnStart": true,
            "beginsPattern": ".",
            "endsPattern": ".",
          }
        }
      ],
      "options": {
        "cwd": "${workspaceFolder}/frontend"
      },
      "group": "build"
    },
  ]
}